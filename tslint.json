{
  "extends": ["tslint:latest", "tslint-react"],
  "linterOptions": {
      "exclude": [
          "node_modules/",
          "server/",
          "client/dist"
      ]
  },
  "rules": {
      "interface-name": false,
      "indent": false,
      "object-literal-sort-keys": false,
      "ban-types": false,
      "quotemark": false,
      "ordered-imports": false,
      "no-submodule-imports": false,
      "max-line-length": false
  }
}